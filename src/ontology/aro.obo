is_a: ARO:3000082 ! antibiotic biosynthesis

[Term]
id: NCIT:85540
name: microbial susceptibility test
namespace: antibiotic_resistance
def: "A method to determine whether microbial growth is affected by an antimicrobial compound" []
is_a: ARO:3000045 ! component of AMR phenotype terminology

[Term]
id: ARO:3004378
name: QepA3
namespace: antibiotic_resistance
def: "A plasmid-mediate quinolone efflux pump described in Klebsiella aerogenes and Escherichia coli. QepA3 confers resistance to fluoroquinolone antibiotics by expelling the antibiotic molecules from the cell. This is distinct from the usual chromosomal mutation mechanism of quinolone resistance. Described by Wang et al. 2017." [PMID:26947590]
is_a: ARO:0010002 ! major facilitator superfamily (MFS) antibiotic efflux pump
relationship: confers_resistance_to_drug_class ARO:0000001 ! fluoroquinolone antibiotic

[Term]
id: ARO:3004379
name: QepA4
namespace: antibiotic_resistance
def: "A plasmid-mediated quinolone efflux pump variant described in Escherichia coli. QepA4 confers resistance to quinolone and fluoroquinolone antibiotics by expulsion from the cell. Described by Manageiro et al. 2017." [PMID:29062302]
is_a: ARO:0010002 ! major facilitator superfamily (MFS) antibiotic efflux pump
relationship: confers_resistance_to_drug_class ARO:0000001 ! fluoroquinolone antibiotic

[Term]
id: ARO:3004380
